<div class="style-div">
      <table id="mock-draft-table">
       <caption>Fantasy Mock Draft</caption>
       <thead>
          <tr>
            <th></th>
            <th>Round 1</th>
            <th>Round 2</th>
            <th>Round 3</th>
            <th>Round 4</th>
            <th>Round 5</th>
            <th>Round 6</th>
            <th>Round 7</th>
            <th>Round 8</th>
            <th>Round 9</th>
            <th>Round 10</th>
            <th>Round 11</th>
            <th>Round 12</th>
          </tr>
       </thead>
       <tbody>
          <tr></tr>
          <tr></tr>
          <tr></tr>
          <tr></tr>
       </tbody>
      </table>
    </div>
         <h2 id="que">Available Players</h2>
      <section>
        <div class="queve-table">

          <table class="queve">
            <caption>Available Players</caption>
            <tbody>
                <% Player.all.each do |player| %>
            <tr>
              <td id="rank"><%= player.rank %></td>
              <td id="name"><%= player.first_name %> <%= player.last_name %></td>
              <td id="position"><%= player.position %></td>
              <td><button id="<%= player.id %>" class="xsmall-button">pick</button></td>
            </tr>
            <% end %>
          </tbody>
          </table>
        </div>
      </section>

      <h2 id="picked-players">Picked Players</h2>
       <section>
        <div class="queve-table">
          <table class="picked">
            <caption>Picked Players</caption>
            <tbody>
            <tr>
              <td>Rank</td>
              <td>Name</td>
              <td>Position</td>
            </tr>
          </tbody>
          </table>
        </div>
      </section>


